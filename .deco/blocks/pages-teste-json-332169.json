    {
      "__resolveType": "site/sections/Rolex/Text.tsx",
      "screen": "S",
      "hexa": "f9f7f4",
      "NotisH1": true,
      "paddingTop": true,
      "paddingBottom": true,
      "title": "MONTAGEM DA CAIXA",
      "text": "<p class=\"CorpsA\"><span class=\"Aucun\"><span style=\"mso-ansi-language: PT-BR;\" data-mce-style=\"mso-ansi-language: PT-BR;\">Uma vez restaurados e limpos, os elementos da caixa são remontados, e as juntas de vedação são substituídas. A impermeabilidade da caixa é então testada.</span></span></p>"
    },
    {
      "__resolveType": "site/sections/Rolex/Gallery.tsx",
      "screen": "S",
      "hexa": "f9f7f4",
      "images": [
